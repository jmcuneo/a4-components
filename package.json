{
  "name": "taskwave",
  "version": "1.0.0",
  "description": "A fast and efficient todo list application built with Bun.",
  "main": "src/server/server.js",
  "scripts": {
    "start": "bun src/server/server.js",
    "dev": "concurrently \"bun --watch src/server/server.js\" \"vite --config vite.config.js\"",
    "dev:frontend": "vite --config vite.config.js",
    "build": "vite build --config vite.config.js",
    "serve": "bun src/server/server.js"
  },
  "author": "William Huang",
  "license": "MIT",
  "dependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "chalk": "4.1.2",
    "concurrently": "^7.0.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "mime": "^2.4.4",
    "mongoose": "^8.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "uuid": "^9.0.1",
    "vite": "^5.2.8"
  }
}
